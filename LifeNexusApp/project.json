            "dependencies": [
                {
                    "package": "supabase-swift",
                    "product": "Supabase"
                },
                {
                    "package": "firebase-ios-sdk",
                    "product": "FirebaseAI"
                },
                {
                    "package": "lottie-spm",
                    "product": "Lottie"
                }
            ]
        }
    },
    "packages": {
        "supabase-swift": {
            "url": "https://github.com/supabase/supabase-swift",
            "from": "2.0.0"
        },
        "firebase-ios-sdk": {
            "url": "https://github.com/firebase/firebase-ios-sdk",
            "from": "11.0.0"
        },
        "lottie-spm": {
