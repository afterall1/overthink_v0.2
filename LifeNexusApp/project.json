{
    "sourceRoot": "LifeNexus",
    "name": "LifeNexus",
    "targets": {
        "LifeNexus": {
            "type": "app",
            "platform": "iOS",
            "deploymentTarget": "17.0",
            "sources": [
                "LifeNexus"
            ],
            "resources": [
                "LifeNexus/Resources"
            ],
            "info": {
                "CFBundleName": "LifeNexus",
                "CFBundleDisplayName": "LifeNexus",
                "CFBundleIdentifier": "com.lifenexus.app",
                "CFBundleShortVersionString": "1.0.0",
                "CFBundleVersion": "1",
